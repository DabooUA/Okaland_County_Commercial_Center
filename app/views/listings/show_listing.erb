<h1> Your New Listing: </h1>
<h2 style="text-decoration: underline;"><%= @listing.title %></h2>
<p>
    <ul>
        <li><%= @listing.year %></li>
        <li><%= @listing.make %></li>
        <li><%= @listing.model %></li>
        <li><%= @listing.miles %> miles</li>
        <li><%= @listing.engine %></li>
        
    </ul>
</p>

<img src="/images/listings/<%= @listing.id%>.jpg">

<p><%= @listing.content %></p>

<form method="POST" action="/listings/<%= @listing.id %>/delete">
    <input id="hidden" type="hidden" name="_method" value="Delete">
    <input type="submit" value="Delete Listing">

</form><br>



<a href="/listings"><input type="button" value="Home"> </a>
<a href="/listings/<%= @listing.id %>/edit"><input type="button" value="Edit Listing"> </a>





