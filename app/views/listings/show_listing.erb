<h2> Your New Listing: </h2>
<p><%= @listings.title %></p>
<p><%= @listings.year %></p>
<p><%= @listings.make %></p>
<p><%= @listings.model %></p>
<p><%= @listings.miles %></p>
<p><%= @listings.content %></p>

<form method="POST" action="/listings/<%= @listing.id %>/delete">
    <input id="hidden" type="hidden" name="_method" value="Delete">
    <input type="submit" value="Delete Listing">
</form>

<a href="/listings/<%= @listing.id %>/edit"> Edit Listing </a>
